{
  "name": "valorantsound",
  "displayName": "ValorantSound",
  "description": "根据valorant击杀音效来生成声音",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "scm": [
        {
          "type": "webview",
          "id": "ValorantSound.vsound",
          "name": "ValorantSound"
        }
      ]
    },
    "commands": [
      {
        "command": "valorantsound.showView",
        "title": "valorantsound:showView"
      }
    ],
    "configuration": {
      "title": "valorantsound",
      "properties": {
        "valorantsound.select": {
          "type": "string",
          "default": "盖亚",
          "enum": [
            "盖亚",
            "混沌"
          ],
          "enumDescriptions": [
            "选择盖亚声音",
            "选择混沌声音"
          ],
          "markdownDescription": "选择声音主题"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "p  e": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}